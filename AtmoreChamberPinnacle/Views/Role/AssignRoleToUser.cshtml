@*@model Microsoft.AspNet.Identity.EntityFramework.IdentityUser*@

@{
    ViewBag.Title = "AssignRoleToUser";
}

<h2>AssignRoleToUser</h2>

@* creates a form to populate a dropdown list *@

@using (Html.BeginForm())
{

    <div class="form-group">
        @Html.Label("Select Your User Type", new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.DropDownList("UserName")
        </div>
    </div>
    <input type="submit" value="Create Role" />
}